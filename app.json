{
  "expo": {
    "name": "PinPoint",
    "slug": "PinPoint",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Appicon.png",
    "scheme": "testapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "bundleIdentifier": "com.voodoo.pinpoint",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to your camera."
      }
    },
    // "ios": {
      
    //   "supportsTablet": true
      
      
    // },
    
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/AppIcon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
